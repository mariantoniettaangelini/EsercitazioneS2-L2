@model Es_Console.CV
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome @Model.InfoPersonali.Nome</h1>
    <table class="table table-hover">
        <thead class="thead-light">
            <tr>
                <th>NOME</th>
                <th>COGNOME</th>
                <th>TELEFONO</th>
                <th>EMAIL</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.InfoPersonali.Nome</td>
                <td>@Model.InfoPersonali.Cognome</td>
                <td>@Model.InfoPersonali.Tel</td>
                <td>@Model.InfoPersonali.Email</td>
            </tr>
        </tbody>
    </table>
</div>
